    "prepublishOnly": "rimraf lib && npm run build && npm run tsc:d"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/react": "17.0.36",
    "@types/react-dom": "17.0.11",
    "grpc-web": "1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.60.1",
    "tslib": "2.3.1",
    "typescript": "4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/react-grpc-web.git"
  },
  "keywords": [
    "react",
    "hooks",
    "protobuf",
    "grpc",
    "grpc-web"
  ],
