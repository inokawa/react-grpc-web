{
  "name": "react-grpc-web",
  "version": "0.0.0",
  "description": "Typed React hooks for gRPC-web.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "tsc": "tsc -p . --noEmit",
